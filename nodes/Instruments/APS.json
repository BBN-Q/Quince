{
"name": "APS2",
"outputs": ["Quadrature", "Marker1", "Marker2", "Marker3", "Marker4"],
"inputs": ["uW LO"],
"parameters": [
	{"name": "Address", "type": "str", "default": "192.168.4.1"},
	{"name": "Sequence File", "type": "filename", "tip": "Save to this filename."},
	{"name": "Master", "type": "boolean", "default": false},
	{"name": "Delay", "matlab_name": "isMaster", "type": "float", "increment": 1e-8, "snap": false, "low": -10.0e-8, "high": 10.0e-8, "default": 0.0, "tip": "sec"},
	{"name": "Trigger Interval", "matlab_name": "triggerInterval", "type": "float", "increment": 0.0001, "snap": false, "low": 0, "high": 0.01, "tip": "sec", "default": 0.001},
	{"name": "Trigger Source", "matlab_name": "triggerSource", "type": "combo", "choices": ["Internal", "External", "System"], "default": "External"},
	{"name": "Ch1. Amp",    "matlab_name": "amplitude", "matlab_cat": "chan_1", "type": "float", "increment": 0.1, "snap": false, "low": 0, "high": 1, "default": 0.3},
	{"name": "Ch1. Offset", "matlab_name": "offset", "matlab_cat": "chan_1", "type": "float", "increment": 0.01, "snap": false, "low": -0.5, "high": 0.5, "default": 0.0},
	{"name": "Ch1. Enable", "matlab_name": "enabled", "matlab_cat": "chan_1","type": "boolean", "default": true},
	{"name": "Ch2. Amp",    "matlab_name": "amplitude", "matlab_cat": "chan_2", "type": "float", "increment": 0.1, "snap": false, "low": 0, "high": 1, "default": 0.3},
	{"name": "Ch2. Offset", "matlab_name": "offset", "matlab_cat": "chan_2", "type": "float", "increment": 0.01, "snap": false, "low": -0.5, "high": 0.5, "default": 0.0},
	{"name": "Ch2. Enable", "matlab_name": "enabled", "matlab_cat": "chan_2","type": "boolean", "default": true}
	]
}