{
"name": "Alazar",
"outputs": ["Data"],
"inputs": ["Trigger"],
"parameters": [
	{"name": "Address", "type": "str", "default": "1"},
	{"name": "Mode", "type": "combo", "choices": ["digitizer", "averager"], "default": "digitizer"},
	{"name": "Trigger Delay", "matlab_name": "delayTime", "matlab_cat": "horizontal", "type": "float", "increment": 1e-8, "snap": false, "low": -10.0e-8, "high": 10.0e-8, "default": 0.0},
	{"name": "Sample Rate", "matlab_name": "samplingRate", "matlab_cat": "horizontal", "type": "float", "increment": 100e6, "snap": false, "low":100e6, "high": 1e9, "default": 500e6, "tip": "Samp/sec"},
	{"name": "Scale", "matlab_name": "verticalScale", "matlab_cat": "vertical", "type": "float", "increment": 0.05, "snap": false, "low":0.01, "high": 0.5, "default": 0.1},
	{"name": "Offset", "matlab_name": "verticalOffset", "matlab_cat": "vertical", "type": "float", "increment": 0.1, "snap": false, "low":-0.5, "high": 0.5, "default": 0.0},
	{"name": "Coupling", "matlab_name": "verticalCoupling", "matlab_cat": "vertical", "type": "combo", "choices": ["AC", "DC"], "default": "DC"},
	{"name": "Bandwidth", "matlab_name": "bandwidth", "matlab_cat": "vertical", "type": "combo", "choices": ["Full", "20 MHz"], "default": "Full"},
	{"name": "Samples",  "matlab_name": "recordLength", "matlab_cat": "averager","type": "int", "increment": 64, "snap": false, "low": 64, "high": 4096},
	{"name": "Segments", "matlab_name": "nbrSegments", "matlab_cat": "averager", "type": "int", "increment": 2000, "snap": false, "low": 1, "high": 512000},
	{"name": "Waveforms", "matlab_name": "nbrWaveforms", "matlab_cat": "averager", "type": "int", "increment": 1, "snap": false, "low": 1, "high": 1024},
	{"name": "Round Robins", "matlab_name": "nbrRoundRobins", "matlab_cat": "averager", "type": "int", "increment": 1, "snap": false, "low": 1, "high": 1024},
	{"name": "Trig. Level", "matlab_name": "triggerLevel", "matlab_cat": "trigger", "type": "float", "increment": 0.02, "snap": false, "low":0.01, "high": 0.5, "default": 0.1},
	{"name": "Trig. Source", "matlab_name": "triggerSource", "matlab_cat": "trigger", "type": "combo", "choices": ["A", "B", "Ext"], "default": "Ext"},
	{"name": "Trig. Coupl.", "matlab_name": "triggerCoupling", "matlab_cat": "trigger",  "type": "combo", "choices": ["AC", "DC"], "default": "DC"},
	{"name": "Trig. Slope", "matlab_name": "triggerSlope", "matlab_cat": "trigger", "type": "combo", "choices": ["rising", "falling"], "default": "rising"}
	]
}