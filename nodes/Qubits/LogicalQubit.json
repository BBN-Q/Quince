{
"name": "Qubit",
"outputs": ["Qubit"],
"inputs": ["Qubit Quad.", "Qubit Gate", "Meas. Quad.", "Meas. Gate"],
"parameters": [
	{"name": "Freq.", "type": "float", "increment": 1e9, "snap": false, "low": 0, "high": 20e9, "tip": "Hz"},
	{"name": "Cutoff", "type": "float", "increment": 1, "snap": false, "low": 0, "high": 20, "tip": "Hz"},
	{"name": "Drag Scale", "type": "float", "increment": 1, "snap": false, "low": 0, "high": 20, "tip": "Hz"},
	{"name": "Length", "type": "float", "increment": 1e-8, "snap": false, "low": 1e-8, "high": 20e-8, "tip": "Hz"},
	{"name": "pi2Amp", "type": "float", "increment": 0.1, "snap": false, "low": 0, "high": 1, "tip": "Hz"},
	{"name": "piAmp", "type": "float", "increment": 0.1, "snap": false, "low": 0, "high": 1, "tip": "Hz"},
	{"name": "shape", "type": "combo", "choices": 
		["square", "constant", "arb_axis_drag", "measPulse", "gaussian", "gaussOn", 
			"delay", "drag", "dragGaussOff", "autodyne", "tanh", "dragGaussOn", "gaussOff"], 
		"default": "drag", "tip": "Hz"},
	{"name": "sigma", "type": "float", "increment": 1e-9, "snap": false, "low": 0, "high": 1e-8, "default": 5e-9, "tip": "Hz"}
	]
}